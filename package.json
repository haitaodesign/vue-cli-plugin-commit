  "repository": {
    "type": "git",
    "url": "git+https://github.com/haitaodesign/vue-cli-plugin-commit.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/haitaodesign/vue-cli-plugin-commit/issues"
  },
  "homepage": "https://github.com/haitaodesign/vue-cli-plugin-commit#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-angular": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "2.1.0",
    "husky": "^1.3.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
